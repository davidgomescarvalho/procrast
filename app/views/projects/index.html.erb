<h1>All Projects</h1>

<ul>
<% if @projects.empty? %>
    <p>No projects found, start a project !</p>
    <% else %>
  <% @projects.each do |project| %>
    <li><%= link_to project.name, project_path(project) %></li>
  <% end %>
<% end %>
</ul>

<%= link_to 'New Project', new_project_path(current_user) %>
