<div class="container-title">
  <h1 class="title-project-h1">All Projects</h1>
</div>

<div class="index">
  <ul id="container-ul">
        <% if @projects.empty? %>
          <p class="text-center my-2">No projects found, start a project !</p>
        <% else %>
          <% @projects.each do |project| %>
            <li><%= link_to project.title, project_path(project) %></li>
          <% end %>
        <% end %>
  </ul>
</div>
